<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/page_bg1">

    <com.kidscademy.instruments.quiz.view.PercentDonutView
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_gravity="center"
        app:donutPercent="0.95"
        app:donutBackgroundColor="@color/white_T80"
        app:donutStrokeColor="@color/green_500"
        app:donutStrokeWidth="20dp"
        app:donutTextColor="@color/black"
        app:donutTextSize="40dp" />

</FrameLayout>
